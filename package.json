{
	"name": "vite-plugin-butterfly-effect",
	"version": "1.0.0",
	"description": "A Vite plugin that visualizes React useEffect and state update chains as beautiful butterfly effects",
	"type": "module",
	"main": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"import": "./dist/index.js"
		},
		"./overlay": {
			"types": "./dist/overlay.d.ts",
			"import": "./dist/overlay.js"
		},
		"./runtime": {
			"types": "./dist/runtime.d.ts",
			"import": "./dist/runtime.js"
		}
	},
	"files": [
		"dist"
	],
	"scripts": {
		"build": "rolldown -c && tsc --emitDeclarationOnly",
		"test": "vitest",
		"test:ui": "vitest --ui",
		"test:coverage": "vitest --coverage"
	},
	"keywords": [
		"vite",
		"vite-plugin",
		"react",
		"useEffect",
		"state",
		"visualization",
		"debugging",
		"devtools"
	],
	"author": "",
	"license": "MIT",
	"dependencies": {
		"@babel/generator": "^7.26.0",
		"@babel/parser": "^7.26.0",
		"@babel/traverse": "^7.26.0",
		"@babel/types": "^7.26.0",
		"enhanced-resolve": "^5.18.3"
	},
	"devDependencies": {
		"@biomejs/biome": "^2.3.4",
		"@types/babel__generator": "^7.6.8",
		"@types/babel__traverse": "^7.20.6",
		"@types/node": "^24.10.0",
		"@vitest/coverage-v8": "^4.0.8",
		"@vitest/ui": "^4.0.8",
		"happy-dom": "^20.0.10",
		"rolldown": "^1.0.0-beta.50",
		"typescript": "^5.9.3",
		"vitest": "^4.0.8"
	}
}
